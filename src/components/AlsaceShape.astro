<style>
    @keyframes shape {
        0% {
            /*stroke-dashoffset: 0;*/
            filter: drop-shadow(0 0 3px #67ea94);
        }
        50% {
            filter: drop-shadow(0 0 5px #67ea94);
        }
        100% {
            /*stroke-dashoffset: 200%;*/
            filter: drop-shadow(0 0 3px #67ea94);
        }
    }

    @keyframes circle-ping {
        0% {
            r: 1px;
            opacity: 1;
        }
        75%, 100% {
            r: 5px;
            opacity: 0;
        }
    }

    .alsace-shape polygon {
        /*stroke-dasharray: 120%;*/
        animation: 7s ease-in-out infinite shape;
        stroke: #67ea94;
        stroke-width: 1;
        fill: transparent;
    }

    .alsace-shape circle:nth-child(1) {
        animation-delay: 0s;
    }

    .alsace-shape circle:nth-child(2) {
        animation-delay: 0.2s;
    }

    .alsace-shape circle:nth-child(3) {
        animation-delay: 0.4s;
    }

    .alsace-shape circle:nth-child(4) {
        animation-delay: 0.6s;
    }

    .alsace-shape circle:nth-child(5) {
        animation-delay: 0.8s;
    }

    .alsace-shape circle:nth-child(6) {
        animation-delay: 1.0s;
    }

    .alsace-shape circle:nth-child(7) {
        animation-delay: 1.2s;
    }

    .alsace-shape circle:nth-child(8) {
        animation-delay: 1.4s;
    }

    .alsace-shape circle:nth-child(9) {
        animation-delay: 1.6s;
    }

    .alsace-shape circle:nth-child(10) {
        animation-delay: 1.8s;
    }

    .alsace-shape circle {
        animation: 2s ease-in-out infinite circle-ping;
        fill: #67ea94;
        filter: drop-shadow(0 0 5px #67ea94);
    }
</style>

